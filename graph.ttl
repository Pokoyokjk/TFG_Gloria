<http://www.gsi.upm.es/ontologies/amor/models/mft/ns> a owl:Ontology ;
    rdfs:label "AMOR-MFT Ontology"@en ;
    dcterms:creator "Sergio Muñoz López"@en,
        "Álvaro Carrera Barroso"@en ;
    dcterms:description "AMOR-MFT is an ontology that uses AMOR ontology and MFT SKOS Taxonomy to create required relations to use MFT to define moral annotations."@en ;
    dcterms:publisher <http://www.gsi.upm.es> ;
    dcterms:title "AMOR-MFT Ontology"@en ;
    vann:preferredNamespacePrefix "amor-mft" ;
    vann:preferredNamespaceUri "http://www.gsi.upm.es/ontologies/amor/models/mft/ns#" ;
    rdfs:comment "AMOR-MFT is an ontology that uses AMOR ontology and MFT SKOS Taxonomy to create required relations to use MFT to define moral annotations."@en ;
    owl:versionIRI <http://www.gsi.upm.es/ontologies/amor/models/mft/ns/1.0.0> ;
    owl:versionInfo "1.0.0" .

ns5:hasPolarity a owl:ObjectProperty ;
    rdfs:label "has polarity"@en ;
    rdfs:comment "The polarity is represents the Vice or the Virtue of the MFT Moral Value category."@en ;
    rdfs:domain ns5:MFTValueCategory ;
    rdfs:range ns5:Polarity .

ns5:hasPolarityIntensity a owl:AnnotationProperty,
        owl:DatatypeProperty ;
    rdfs:label "has polarity intensity"@en ;
    rdfs:comment "The polarity of a moral value annotation represent how close is the annotated content to the vice or the virtue. The range of this polarity depends of the Moral Value Model that is used in the analysis. Even, it is not mandatory to associate a polarity to an annotation since annotation can be categorical using the porpoerty hasPolarity."@en ;
    rdfs:domain :MoralValueAnnotation ;
    rdfs:range xsd:float .

ns5:oppositeOf a owl:ObjectProperty ;
    rdfs:domain ns5:Polarity ;
    rdfs:range ns5:Polarity .

<http://www.gsi.upm.es/ontologies/amor/ns> a owl:Ontology ;
    rdfs:label "AMOR Moral Values Ontology"@en ;
    dcterms:creator "Sergio Muñoz López"@en,
        "Álvaro Carrera Barroso"@en ;
    dcterms:description "AMOR is an ontology designed for representing moral values and value annotations in textual sources."@en ;
    dcterms:publisher <http://www.gsi.upm.es> ;
    dcterms:title "AMOR Moral Values Ontology"@en ;
    vann:preferredNamespacePrefix "amor" ;
    vann:preferredNamespaceUri "http://www.gsi.upm.es/ontologies/amor/ns#" ;
    rdfs:comment "AMOR is an ontology designed for representing moral values analysis and annotations in textual sources."@en ;
    owl:versionIRI <http://www.gsi.upm.es/ontologies/amor/ns/1.0.0> ;
    owl:versionInfo "1.0.0" .

:MLEntity a owl:Class ;
    rdfs:label "Machine Learning Entity"@en ;
    rdfs:comment "Represents any entity related with a Machine Learning process. It is a subclass of mls:Entity and prov:Entity."@en ;
    rdfs:subClassOf ns2:InformationEntity,
        prov:Entity .

:MLProcess a owl:Class ;
    rdfs:label "Machine Learning Process"@en ;
    rdfs:comment "Represents a process related with Machine Learning that generates some output, such as a mls:Model or any other mls:InformationEntity."@en ;
    rdfs:subClassOf ns2:Run,
        prov:Activity .

:analysed a owl:AnnotationProperty,
        owl:ObjectProperty ;
    rdfs:label "analysed"@en ;
    rdfs:comment "This property relates a ValueAnalysis with the resource that is analysed."@en ;
    rdfs:domain :MoralValueAnalysis ;
    rdfs:range :AnalysedEntity ;
    rdfs:subPropertyOf prov:used .

:annotated a owl:AnnotationProperty,
        owl:ObjectProperty ;
    rdfs:label "annotated"@en ;
    rdfs:comment "This property relates the ValueAnnotation activity with the resource that is annotated."@en ;
    rdfs:domain :MoralValueAnnotation ;
    rdfs:range :AnalysedEntity ;
    rdfs:subPropertyOf prov:wasDerivedFrom .

:confidence a owl:AnnotationProperty,
        owl:DatatypeProperty ;
    rdfs:label "confidence"@en ;
    rdfs:comment "The confidence score of the moral value annotation, ranging from 0 to 1."@en ;
    rdfs:domain :MoralValueAnnotation ;
    rdfs:range xsd:float .

:hasMoralValueCategory a owl:AnnotationProperty,
        owl:ObjectProperty ;
    rdfs:label "has moral value category"@en ;
    rdfs:comment "Specifies the category of the moral value."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :MoralValueAnnotation :MoralValueModel ) ],
        [ a owl:Class ;
            owl:unionOf ( :MoralValueAnnotation :MoralValueModel ) ],
        [ a owl:Class ;
            owl:unionOf ( :MoralValueAnnotation :MoralValueModel ) ] ;
    rdfs:range :MoralValueCategory ;
    rdfs:subPropertyOf prov:hadMember .

:usedMLModel a owl:AnnotationProperty,
        owl:ObjectProperty ;
    rdfs:label "used machine learning model"@en ;
    rdfs:comment "This property relates an activity (such as a MoralAnalysis ) with the Machine Learning Model (mls:Model) used to carry out the activity."@en ;
    rdfs:domain prov:Activity ;
    rdfs:range ns2:Model ;
    rdfs:subPropertyOf prov:used .

:usedMoralValueModel a owl:AnnotationProperty,
        owl:ObjectProperty ;
    rdfs:label "used moral value model"@en ;
    rdfs:comment "Specifies the moral value model used in the analysis."@en ;
    rdfs:domain :MoralValueAnalysis ;
    rdfs:range :MoralValueModel ;
    rdfs:subPropertyOf prov:used .

