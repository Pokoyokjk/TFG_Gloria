  {
    "kind": 2,
    "language": "sparql",
    "value": "PREFIX prov: <http://www.w3.org/ns/prov#>\r\nPREFIX segb: <http://www.gsi.upm.es/ontologies/segb/ns#>\r\nPREFIX foaf: <http://xmlns.com/foaf/0.1/>\r\nPREFIX mls: <http://www.w3.org/ns/mls#>\r\nPREFIX oro: <http://kb.openrobots.org#>\r\nPREFIX xsd: <http://www.w3.org/2001/XMLSchema#>\r\n\r\n\r\nSELECT ?activity ?secondType ?model ?robot ?speaker ?listener ?text\r\nWHERE {\r\n?activity a segb:LoggedActivity , ?secondType ;\r\n        segb:usedMLModel ?model ;\r\n        segb:wasPerformedBy ?robot ;\r\n        oro:hasMessage ?msg ;\r\n        oro:hasSpeaker ?speaker ;\r\n        oro:hasListener ?listener .\r\n?msg a oro:Message ;\r\n        oro:hasText ?text .\r\nFILTER(?secondType != segb:LoggedActivity)\r\n}",
    "metadata": {}
  },
  {
    "kind": 2,
    "language": "sparql",
    "value": "PREFIX oro: <http://kb.openrobots.org#>\r\nPREFIX segb: <http://www.gsi.upm.es/ontologies/segb/ns#>\r\n\r\nSELECT ?activity ?robot ?trigger ?result ?human ?msg1 ?msg2 ?text1 ?text2\r\nWHERE {\r\n?activity a segb:LoggedActivity ;\r\n    segb:triggeredByActivity ?trigger ;\r\n    segb:producedActivityResult ?result ;\r\n    segb:wasPerformedBy ?robot .\r\n?trigger a oro:ListeningEvent ;\r\n    oro:hasListener ?robot ;\r\n    oro:hasSpeaker ?human ;\r\n    oro:hasMessage ?msg1 .\r\n?msg1 a oro:Message ;\r\n    oro:hasText ?text1 .\r\n?result a oro:SpeechAction ;\r\n    oro:hasSpeaker ?robot ;\r\n    oro:hasListener ?listener ;\r\n    oro:hasMessage ?msg2 .\r\n?msg2 a oro:Message ;\r\n    oro:hasText ?text2 .\r\n}",
    "metadata": {
